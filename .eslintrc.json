{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
      "eslint:recommended", 
      "plugin:react/recommended", 
      "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "2021",
    "sourceType": "module"
  },
 
  "plugins": ["react"],
  "rules": {
    "react/function-component-definition": "off",
    "singleQuote": true
  }
}
